<div class='container'>
	<div class='row'>
		<h2>profile Page</h2>
		<label>Name:</label><%=@user.first_name%> <%=@user.last_name%></br>
		<label>Email:</label><%=@user.email%></br>
		<label>Gender:</label><%=@user.gender%></br>
		<label>Phone:</label><%=@user.phone%></br>
		<label>Profession:</label><%=@user.profession%></br>
		<label>Date Of Birth:</label><%=@user.dob%></br>
		<label>Status:</label><%=@user.status%></br>
		<label>Address:</label><%=@user.address%>
		<%=image_tag @user.attachments%>
		<!-- <label>Description:</label><%=@user%> -->
		<%= form_for @user, :html => {:multipart => true} do |f| %>
		  <p>
		    <%= f.file_field :image, :class => 'form-control'%>
		  </p>
		  <p><%= f.submit %></p>
		<% end %>
	</div>
</di>